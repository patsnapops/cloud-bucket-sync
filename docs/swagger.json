    "definitions": {
        "api.ChangeRecordStatusRequest": {
            "type": "object",
            "properties": {
                "operator": {
                    "type": "string"
                },
                "record_id": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                }
            }
        },
        "model.Record": {
            "type": "object",
            "properties": {
                "cost_time": {
                    "description": "耗时 单位 s",
                    "type": "integer"
                },
                "created_at": {
                    "type": "string"
                },
                "error_http_url": {
                    "description": "错误文件列表的http下载地址",
                    "type": "string"
                },
                "error_s3_url": {
                    "description": "错误文件列表的s3地址",
                    "type": "string"
                },
                "failed_files": {
                    "description": "失败文件数",
                    "type": "integer"
                },
                "id": {
                    "type": "string"
                },
                "info": {
                    "description": "任务信息",
                    "type": "string"
                },
                "is_confirm": {
                    "description": "是否需要确认",
                    "type": "boolean"
                },
                "operator": {
                    "description": "操作人",
                    "type": "string"
                },
                "running_mode": {
                    "description": "运行模式, syncOnce,KeepSync",
                    "type": "string"
                },
                "status": {
                    "description": "任务状态 请走action接口去修改。",
                    "type": "string"
                },
                "task_id": {
                    "description": "任务ID",
                    "type": "string"
                },
                "total_files": {
                    "description": "总文件数",
                    "type": "integer"
                },
                "total_size": {
                    "description": "总大小 单位 B",
                    "type": "integer"
                },
                "updated_at": {
                    "type": "string"
                },
                "worker_id": {
                    "description": "执行任务的workerID",
                    "type": "string"
                }
            }
        },
        "model.Task": {
            "type": "object",
            "required": [
                "is_server_side",
                "name",
                "source_profile",
                "source_url",
                "submitter",
                "sync_mode",
                "target_profile",
                "target_url",
                "worker_tag"
            ],
            "properties": {
                "corn": {
                    "description": "格式为 分、时、日、月、周                                                      // cron表达式 用于定时任务 ’分 时 日 月 周‘",
                    "type": "string",
                    "example": "0 */8 * * 1,2,3,4,5"
                },
                "created_at": {
                    "type": "string"
                },
                "exclude": {
                    "description": "排除",
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "include": {
                    "description": "包含",
                    "type": "string"
                },
                "is_deleted": {
                    "type": "boolean"
                },
                "is_server_side": {
                    "description": "是否跨区域,默认启用，决定是否流量经过本地，涉及到流量费用",
                    "type": "boolean"
                },
                "is_silence": {
                    "description": "是否静默 默认false 静默不发送通知",
                    "type": "boolean",
                    "example": false
                },
                "keys_url": {
                    "description": "S3URL s3://bucket/key 支持提供文件列表去同步",
                    "type": "string",
                    "example": "s3://bucket/key"
                },
                "meta": {
                    "description": "任务元信息",
                    "type": "string",
                    "example": "Expires:2022-10-12T00:00:00.000Z#Cache-Control:no-cache#Content-Encoding:gzip#x-cos-meta-x:x"
                },
                "name": {
                    "description": "任务名称",
                    "type": "string"
                },
                "source_profile": {
                    "description": "源Profile配置 可选 cn9554,cn3977,cn0536,us7478,us0066,us1549,tx-cn,tx-us",
                    "type": "string",
                    "default": "proxy",
                    "example": "cn3977"
                },
                "source_url": {
                    "description": "S3URL s3://sourceBucket/key 支持文件和目录结尾",
                    "type": "string"
                },
                "storage_class": {
                    "description": "存储类型 STANDARD,STANDARD_IA,ONEZONE_IA,INTELLIGENT_TIERING,REDUCED_REDUNDANCY,STANDARD_IA,ONEZONE_IA,INTELLIGENT_TIERING,REDUCED_REDUNDANCY",
                    "type": "string"
                },
                "submitter": {
                    "description": "提交人",
                    "type": "string"
                },
                "sync_mode": {
                    "description": "默认运行模式 syncOnce 一次性任务, KeepSync 持续同步",
                    "type": "string",
                    "example": "syncOnce"
                },
                "target_profile": {
                    "description": "目标Profile配置 可选 cn9554,cn3977,cn0536,us7478,us0066,us1549,tx-cn,tx-us",
                    "type": "string",
                    "default": "proxy",
                    "example": "us7478"
                },
                "target_url": {
                    "description": "S3URL s3://destBucket/dir/ 不支持文件结尾 没有/的目录看作目录处理",
                    "type": "string"
                },
                "time_after": {
                    "description": "在某个时间之后 UTC时间格式：“1992-03-01 21:26:30”",
                    "type": "string",
                    "example": "1992-03-01 21:26:30"
                },
                "time_before": {
                    "description": "在某个时间之前 UTC时间格式：“1992-03-01 21:26:30”",
                    "type": "string",
                    "example": "1992-03-01 21:26:30"
                },
                "updated_at": {
                    "type": "string"
                },
                "worker_tag": {
                    "description": "任务执行节点, 用于标记任务归属于哪个worker,会涉及到费用，需要注意选择正确的workerTag。借鉴与gitlab CICD runner.",
                    "type": "string"
                }
            }
        },
        "model.TaskExecInput": {
            "type": "object",
            "properties": {
                "operator": {
                    "type": "string"
                },
                "sync_mode": {
                    "description": "执行模式，支持修改同步模式。keepSync（实时同步） syncOnce（一次同步）",
                    "type": "string"
                },
                "task_id": {
                    "type": "string"
                }
            }
        },
        "model.WorkerRequest": {
            "type": "object",
            "properties": {
                "cloud": {
                    "type": "string"
                },
                "region": {
                    "type": "string"
                }
            }
        },
        "model.WorkerResponse": {
            "type": "object",
            "properties": {
                "cloud": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string"
                },
                "hc": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "region": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                }
            }
        }
    }
}